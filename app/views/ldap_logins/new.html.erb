<h1>LdapLogins#new</h1>
<p>Find me in app/views/ldap_logins/new.html.erb</p>


<%= form_tag(ldap_login_path) do %>
  <% [:client_id, :state].each do |k| %>
    <%= hidden_field_tag k, params[k] %>
  <% end %>

  <%= label_tag(:login, "Login") %>
  <%= text_field_tag(:login) %>
  <br>
  <%= label_tag(:password, "Password") %>
  <%= password_field_tag(:password) %>
  <%= submit_tag("Log in") %>
<% end %>
